<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="Tele_StationBase">
		<defName>AtmosphericPump</defName>
		<label>Atmospheric Pump</label>
		<description>.</description>
		<thingCategories Inherit="false" />
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<drawerType>MapMeshAndRealTime</drawerType>
		<designationCategory>Atmospheric</designationCategory>
		<graphicData>
			<texPath>Things/Building/Power/WoodFiredGenerator</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(2,2)</drawSize>
		</graphicData>
		<statBases>
			<MaxHitPoints>200</MaxHitPoints>
			<WorkToBuild>1000</WorkToBuild>
			<Flammability>1.0</Flammability>
			<Beauty>-5</Beauty>
		</statBases>
		<size>(2,2)</size>
		<costList>
			<Steel>100</Steel>
			<ComponentIndustrial>5</ComponentIndustrial>
		</costList>
		<comps>
			<li Class="TeleCore.CompProperties_Network">
				<!--<compClass>TiberiumRim.Comp_ANS_Filter</compClass>-->
				<networks>
					<li>
						<networkDef>AtmosphericNetwork</networkDef>
						<roles>
							<li>Transmitter</li>
							<li>Controller</li>
							<li>Storage</li>
						</roles>
						<volumeConfig>
							<capacity>1000</capacity>
							<area>1</area>
							<elevation>0</elevation>
							<height>1</height>
							<!--
							<baseCapacity>1000</baseCapacity>
							<dropContents>false</dropContents>
							<valueDefs>
								<fromCollection>AtmosphericNetwork</fromCollection>
							</valueDefs>
							-->
						</volumeConfig>
					</li>
				</networks>
				<generalIOConfig>
					<patternSize>(4,4)</patternSize>
					<pattern>#XX#|X++X|X++X|#XX#</pattern>
				</generalIOConfig>
			</li>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>1000</basePowerConsumption>
			</li>
		</comps>
		<rotatable>true</rotatable>
		<tickerType>Never</tickerType>
		<castEdgeShadows>false</castEdgeShadows>
		<staticSunShadowHeight>0</staticSunShadowHeight>	
		<fillPercent>1</fillPercent>			
		<surfaceType>Item</surfaceType>
		<modExtensions>
			<li Class="TeleCore.SubMenuExtension">
				<groupDef>TestGroup</groupDef>
				<category>Structure</category>
			</li>
		</modExtensions>
	</ThingDef>
	
	<ThingDef ParentName="NetworkPipeBase">
		<defName>AtmosphericPipe</defName>
		<label>gas pipe</label>
		<description>This structure connects Atmospheric processing structures on the Atmospheric Network.</description>
		<building>
			<blueprintGraphicData>
				<texPath>Buildings/Network/Pipe_Air_blueprint</texPath>
				<graphicClass>TeleCore.Graphic_LinkedWithSame</graphicClass>
			</blueprintGraphicData>
		</building>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToBuild>100</WorkToBuild>
			<Flammability>0.2</Flammability>
			<Beauty>-18</Beauty>
		</statBases>
		<costList>
			<Steel>5</Steel>
		</costList>
		<comps>
			<li Class="TeleCore.CompProperties_Network">
				<networks>
					<li>
						<networkDef>AtmosphericNetwork</networkDef>
						<roles>
							<li>Transmitter</li>
						</roles>
					</li>
				</networks>
			</li>
		</comps>
		<modExtensions>
			<li Class="TeleCore.SubMenuExtension">
				<groupDef>TestGroup</groupDef>
				<category>Structure</category>
			</li>
		</modExtensions>
	</ThingDef>
	
	<ThingDef ParentName="Tele_StationBase">
		<defName>AtmosphericVentIn</defName>
		<label>atmospheric vent (intake)</label>
		<description>.</description>
		<thingCategories Inherit="false" />
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<drawerType>MapMeshAndRealTime</drawerType>
		<tickerType>Normal</tickerType>
		<uiIconPath>Buildings/Network/Intake/icon</uiIconPath>
		<designationCategory>Atmospheric</designationCategory>
		<graphicData>
			<texPath>Buildings/Network/Intake/base</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(1.25,1.25)</drawSize>
			<drawOffset>(0,0,0.1)</drawOffset>
		</graphicData>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToBuild>1500</WorkToBuild>
			<Flammability>1.0</Flammability>
			<Beauty>-10</Beauty>
		</statBases>
		<size>(1,1)</size>
		<costList>
			<Steel>50</Steel>
			<ComponentIndustrial>1</ComponentIndustrial>
		</costList>
		<comps>
			<li Class="TAE.CompProperties_ANS_AirVent">
				<ventMode>Intake</ventMode>
				<gasThroughPut>10</gasThroughPut>
				<filter>
					<acceptedTag>Air</acceptedTag>
				</filter>
				<networks>
					<li>
						<networkDef>AtmosphericNetwork</networkDef>
						<roles>
							<li>Transmitter</li>
							<li>Producer</li>
						</roles>
						<volumeConfig>
							<capacity>100</capacity>
							<area>1</area>
							<elevation>0</elevation>
							<height>1</height>
							<!--
							<baseCapacity>100</baseCapacity>
							<dropContents>false</dropContents>
							<valueDefs>
								<fromCollection>AtmosphericNetwork</fromCollection>
							</valueDefs>
							-->
						</volumeConfig>
					</li>
				</networks>
				<generalIOConfig>
					<patternSize>(3,3)</patternSize>
					<pattern>#O#|O#O|#O#</pattern>
				</generalIOConfig>
			</li>
			<li Class="TeleCore.CompProperties_FX"> 
				<fxLayers>
					<li>
						<categoryTag>AirVent</categoryTag>
						<layerTag>AirVentRotationLayer</layerTag>
						<fxMode>Dynamic</fxMode>
						<graphicData>
							<texPath>Buildings/Network/Intake/fan</texPath>
							<graphicClass>Graphic_Single</graphicClass>
							<drawSize>(1.25,1.25)</drawSize>
							<drawOffset>(0,0,0.1)</drawOffset>
						</graphicData>
						<rotate>
							<rotationSpeed>75</rotationSpeed>
						</rotate>
						<needsPower>false</needsPower>
					</li>
					<li>
						<categoryTag>AirVent</categoryTag>
						<graphicData>
							<texPath>Buildings/Network/Intake/top</texPath>
							<graphicClass>Graphic_Single</graphicClass>
							<drawSize>(1.25,1.25)</drawSize>
							<drawOffset>(0,0,0.1)</drawOffset>
						</graphicData>
						<needsPower>false</needsPower>
					</li>
				</fxLayers>
			</li>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>45</basePowerConsumption>
			</li>
		</comps>
		<placeWorkers>
			<li>TAE.PlaceWorker_AtmosphericVent</li>
		</placeWorkers>
		<castEdgeShadows>false</castEdgeShadows>
		<staticSunShadowHeight>0</staticSunShadowHeight>	
		<fillPercent>1</fillPercent>
		<rotatable>true</rotatable>			
		<surfaceType>Item</surfaceType>
		<modExtensions>
			<li Class="TeleCore.SubMenuExtension">
				<groupDef>TestGroup</groupDef>
				<category>Structure</category>
			</li>
		</modExtensions>
	</ThingDef>
	
	<ThingDef ParentName="Tele_StationBase">
		<defName>AtmosphericVentOut</defName>
		<label>atmospheric vent (out)</label>
		<description>.</description>
		<thingCategories Inherit="false" />
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<drawerType>MapMeshAndRealTime</drawerType>
		<tickerType>Normal</tickerType>
		<size>(1,1)</size>
		<designationCategory>Atmospheric</designationCategory>
		<graphicData>
			<texPath>Buildings/Network/Vent_Out</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(1,1)</drawSize>
		</graphicData>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToBuild>1500</WorkToBuild>
			<Flammability>1.0</Flammability>
			<Beauty>-10</Beauty>
		</statBases>
		<costList>
			<Steel>50</Steel>
			<ComponentIndustrial>1</ComponentIndustrial>
		</costList>
		<comps>
			<li Class="TAE.CompProperties_ANS_AirVent">
				<ventMode>Output</ventMode>
				<gasThroughPut>10</gasThroughPut>
				<filter>
					<acceptedTag>Air</acceptedTag>
				</filter>
				<networks>
					<li>
						<networkDef>AtmosphericNetwork</networkDef>
						<roles>
							<li>Transmitter</li>
							<li>Requester</li>
						</roles>
						<volumeConfig>
							<capacity>100</capacity>
							<area>1</area>
							<elevation>0</elevation>
							<height>1</height>
							<!--
							<baseCapacity>100</baseCapacity>
							<dropContents>false</dropContents>
							<valueDefs>
								<fromCollection>AtmosphericNetwork</fromCollection>
							</valueDefs>
							-->
						</volumeConfig>
					</li>
				</networks>
				<generalIOConfig>
					<patternSize>(3,3)</patternSize>
					<pattern>#I#|I#I|#I#</pattern>
				</generalIOConfig>
			</li>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>45</basePowerConsumption>
			</li>
		</comps>
		<placeWorkers>
			<li>TAE.PlaceWorker_AtmosphericVent</li>
		</placeWorkers>
		<castEdgeShadows>false</castEdgeShadows>
		<staticSunShadowHeight>0</staticSunShadowHeight>	
		<fillPercent>1</fillPercent>
		<rotatable>true</rotatable>			
		<surfaceType>Item</surfaceType>
		<modExtensions>
			<li Class="TeleCore.SubMenuExtension">
				<groupDef>TestGroup</groupDef>
				<category>Structure</category>
			</li>
		</modExtensions>
	</ThingDef>
	
	
	<!-- Passive Atmospheric Exchange -->
	<ThingDef ParentName="Tele_BuildingBaseOverBuildings">
		<defName>TAE_PassiveVent</defName>
		<label>passive atmospheric vent</label>
		<description>.</description>
		<thingClass>Building</thingClass>
		<tickerType>Normal</tickerType>
		<designationCategory>Atmospheric</designationCategory>
		<altitudeLayer>BuildingOnTop</altitudeLayer>
		<costList>
			<Steel>30</Steel>
		</costList>
		<statBases>
			<WorkToBuild>400</WorkToBuild>
			<MaxHitPoints>100</MaxHitPoints>
			<Flammability>1.0</Flammability>
		</statBases>
		<graphicData>
			<texPath>Buildings/Network/PassiveVent/Vent</texPath>
			<graphicClass>TeleCore.Graphic_MultiWithAlt</graphicClass>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<comps>
			<li Class="TAE.CompProperties_ANS_Vent">
				<compClass>TAE.Comp_ANS_PassiveVent</compClass>
				<intakeOffset>(0,0,-1)</intakeOffset>
				<ventMode>TwoWay</ventMode>
				<gasThroughPut>10</gasThroughPut>
				<filter>
					<acceptedTag>Air</acceptedTag>
				</filter>
				<networks>
					<li>
						<networkDef>AtmosphericNetwork</networkDef>
						<requiresController>false</requiresController>
						<roles>
							<li>Transmitter</li>
							<li>Storage</li>
						</roles>
						<volumeConfig>
							<capacity>400</capacity>
							
							<!--
							<baseCapacity>400</baseCapacity>
							<dropContents>false</dropContents>
							<storeEvenly>true</storeEvenly>
							<valueDefs>
								<fromCollection>AtmosphericNetwork</fromCollection>
							</valueDefs>
							-->
						</volumeConfig>
					</li>
				</networks>
				<generalIOConfig>
					<patternSize>(3,3)</patternSize>
					<pattern>#+#|+#+|#+#</pattern>
				</generalIOConfig>
			</li>
			<li Class="CompProperties_Flickable">
				<commandTexture>UI/Commands/Vent</commandTexture>
				<commandLabelKey>CommandDesignateOpenCloseVentLabel</commandLabelKey>
				<commandDescKey>CommandDesignateOpenCloseVentDesc</commandDescKey>
			</li>
		</comps>
		<placeWorkers>
			<li>TAE.PlaceWorker_AtmosphericVent</li>
		</placeWorkers>
		<modExtensions>
			<li Class="TeleCore.TeleDefExtension">
				<graphicAlternateWorkerType>TeleCore.AlternateGraphicWorkerWallOnSouth</graphicAlternateWorkerType>
			</li>
			<li Class="TeleCore.SubMenuExtension">
				<groupDef>TestGroup</groupDef>
				<category>Structure</category>
			</li>
		</modExtensions>
	</ThingDef>
	
</Defs>