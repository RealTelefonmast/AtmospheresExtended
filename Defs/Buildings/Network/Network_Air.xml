	<ThingDef ParentName="Tele_BuildingBaseOverBuildings">
		<defName>TAE_PoweredVent</defName>
		<label>atmospheric vent (powered)</label>
		<description>.</description>
		<thingClass>Building</thingClass>
		<tickerType>Normal</tickerType>
		<designationCategory>Atmospheric</designationCategory>
		<altitudeLayer>BuildingOnTop</altitudeLayer>
		<costList>
			<Steel>30</Steel>
		</costList>
		<statBases>
			<WorkToBuild>400</WorkToBuild>
			<MaxHitPoints>100</MaxHitPoints>
			<Flammability>1.0</Flammability>
		</statBases>
		<graphicData>
			<texPath>Buildings/Network/PassiveVent/Vent</texPath>
			<graphicClass>TeleCore.Graphic_MultiWithAlt</graphicClass>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<comps>
			<li Class="TAE.CompProperties_ANS_Vent">
				<compClass>TAE.Comp_ANS_Vent</compClass>
				<passive>false</passive>
				<intakeOffset>(0,0,-1)</intakeOffset>
				<ventMode>TwoWay</ventMode>
				<gasThroughPut>10</gasThroughPut>
				<filter>
					<acceptedTag>Air</acceptedTag>
				</filter>
				<networks>
					<li>
						<networkDef>AtmosphericNetwork</networkDef>
						<requiresController>false</requiresController>
						<roles>
							<li>Transmitter</li>
							<li>Storage</li>
						</roles>
						<volumeConfig>
							<capacity>80</capacity>
							<values>
								<fromCollection>AtmosphericNetwork</fromCollection>
							</values>
						</volumeConfig>
					</li>
				</networks>
				<generalIOConfig>
					<pattern>#X#|X+X|#X#</pattern>
				</generalIOConfig>
			</li>
			<!--
			<li Class="CompProperties_Flickable">
				<commandTexture>UI/Commands/Vent</commandTexture>
				<commandLabelKey>CommandDesignateOpenCloseVentLabel</commandLabelKey>
				<commandDescKey>CommandDesignateOpenCloseVentDesc</commandDescKey>
			</li>
			-->
		</comps>
		<placeWorkers>
			<li>TAE.PlaceWorker_AtmosphericVent</li>
		</placeWorkers>
		<modExtensions>
			<li Class="TeleCore.TeleDefExtension">
				<graphicAlternateWorkerType>TeleCore.AlternateGraphicWorkerWallOnSouth</graphicAlternateWorkerType>
			</li>
			<li Class="TeleCore.SubMenuExtension">
				<groupDef>TestGroup</groupDef>
				<category>Structure</category>
			</li>
		</modExtensions>
	</ThingDef>
	
	
