<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="Tele_StationBase">
		<defName>AtmosphericPump</defName>
		<label>Atmospheric Pump</label>
		<description>.</description>
		<thingCategories Inherit="false" />
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<drawerType>MapMeshAndRealTime</drawerType>
		<designationCategory>Atmospheric</designationCategory>
		<graphicData>
			<texPath>Things/Building/Power/WoodFiredGenerator</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(2,2)</drawSize>
		</graphicData>
		<statBases>
			<MaxHitPoints>200</MaxHitPoints>
			<WorkToBuild>1000</WorkToBuild>
			<Flammability>1.0</Flammability>
			<Beauty>-5</Beauty>
		</statBases>
		<size>(2,2)</size>
		<costList>
			<Steel>100</Steel>
			<ComponentIndustrial>5</ComponentIndustrial>
		</costList>
		<comps Inherit="False">
			<li Class="TeleCore.CompProperties_Network">
				<!--<compClass>TiberiumRim.Comp_ANS_Filter</compClass>-->
				<networks>
					<li>
						<networkDef>AtmosphericNetwork</networkDef>
						<roles>
							<li>Transmitter</li>
							<li>Controller</li>
							<li>Storage</li>
						</roles>
						<volumeConfig>
							<capacity>1000</capacity>
							<area>1</area>
							<elevation>0</elevation>
							<height>1</height>
							<values>
								<fromCollection>AtmosphericNetwork</fromCollection>
							</values>
						</volumeConfig>
					</li>
				</networks>
				<generalIOConfig>
					<pattern>#XX#|X++X|X++X|#XX#</pattern>
				</generalIOConfig>
			</li>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>1000</basePowerConsumption>
			</li>
		</comps>
		<rotatable>true</rotatable>
		<tickerType>Never</tickerType>
		<castEdgeShadows>false</castEdgeShadows>
		<staticSunShadowHeight>0</staticSunShadowHeight>	
		<fillPercent>1</fillPercent>			
		<surfaceType>Item</surfaceType>
		<modExtensions>
			<li Class="TeleCore.SubMenuExtension">
				<groupDef>TestGroup</groupDef>
				<category>Structure</category>
			</li>
		</modExtensions>
	</ThingDef>
	
	<ThingDef ParentName="NetworkPipeBase">
		<defName>AtmosphericPipe</defName>
		<label>gas pipe</label>
		<description>This structure connects Atmospheric processing structures on the Atmospheric Network.</description>
		<building>
			<blueprintGraphicData>
				<texPath>Buildings/Network/Pipe_Air_blueprint</texPath>
				<graphicClass>TeleCore.Graphic_LinkedWithSame</graphicClass>
			</blueprintGraphicData>
		</building>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToBuild>100</WorkToBuild>
			<Flammability>0.2</Flammability>
			<Beauty>-18</Beauty>
		</statBases>
		<costList>
			<Steel>5</Steel>
		</costList>
		<comps>
			<li Class="TeleCore.CompProperties_Network">
				<networks>
					<li>
						<networkDef>AtmosphericNetwork</networkDef>
						<roles>
							<li>Transmitter</li>
						</roles>
						<volumeConfig>
							<capacity>400</capacity>
							<area>1</area>
							<elevation>0</elevation>
							<height>1</height>
							<values>
								<fromCollection>AtmosphericNetwork</fromCollection>
							</values>
						</volumeConfig>
					</li>
				</networks>
			</li>
		</comps>
		<modExtensions>
			<li Class="TeleCore.SubMenuExtension">
				<groupDef>TestGroup</groupDef>
				<category>Structure</category>
			</li>
		</modExtensions>
	</ThingDef>
	
	<ThingDef ParentName="Tele_StationBase">
		<defName>AtmosphericValve</defName>
		<label>valve</label>
		<description>.</description>
		<size>(1,1)</size>
		<costList>
			<Steel>100</Steel>
		</costList>
		<receivesSignals>true</receivesSignals>
		<rotatable>true</rotatable>
		<uiIconPath>Buildings/Network/Valve/Valve_Icon</uiIconPath>
		<graphicData>
			<texPath>Buildings/Network/Valve/Valve_Base</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(1.25,1.25)</drawSize>
			<drawRotated>false</drawRotated>
		</graphicData>
		<drawerType>MapMeshAndRealTime</drawerType>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<pathCost>70</pathCost>
		<blockWind>true</blockWind>
		<castEdgeShadows>false</castEdgeShadows>
		<staticSunShadowHeight>0</staticSunShadowHeight>
		<fillPercent>1</fillPercent>
		<tickerType>Never</tickerType>
		<statBases>
			<MaxHitPoints>350</MaxHitPoints>
			<WorkToBuild>2500</WorkToBuild>
			<Mass>20</Mass>
			<Flammability>1.0</Flammability>
			<Beauty>0</Beauty>
		</statBases>
		<comps>
			<li Class="TeleCore.CompProperties_Network">
				<compClass>TeleCore.Comp_Network_Valve</compClass>
				<networks>
					<li>
						<networkDef>AtmosphericNetwork</networkDef>
						<roles>
							<li>Transmitter</li>
							<li>Storage</li>
						</roles>
						<volumeConfig>
							<capacity>400</capacity>
							<area>1</area>
							<elevation>0</elevation>
							<height>1</height>
							<values>
								<fromCollection>AtmosphericNetwork</fromCollection>
							</values>
						</volumeConfig>
					</li>
				</networks>
				<generalIOConfig>
					<pattern>###|I+O|###</pattern>
				</generalIOConfig>
			</li>
			<li Class="TeleCore.CompProperties_FX"> 
				<fxLayers>
					<li>
						<graphicData>
							<texPath>Buildings/Network/Valve/Valve_Handle</texPath>
							<graphicClass>Graphic_Single</graphicClass>
							<drawSize>(1.25,1.25)</drawSize>
						</graphicData>
						<pivotPixelOffset>(0,0,5)</pivotPixelOffset>
						<textureSize>(64,64)</textureSize>
						<fxMode>Dynamic</fxMode>
						<categoryTag>FXNetwork</categoryTag>
						<layerTag>Valve</layerTag>
					</li>
				</fxLayers>
			</li>
			<li Class="CompProperties_Flickable">
				<commandTexture>Buildings/Network/Valve/Valve_Handle</commandTexture>
				<commandLabelKey>TAE.Network.Valve.Label</commandLabelKey>
				<commandDescKey>TAE.Network.Valve.Desc</commandDescKey>
			</li>
		</comps>
		<terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
		<constructEffect>ConstructMetal</constructEffect>
		<canOverlapZones>false</canOverlapZones>
		<modExtensions>
			<li Class="TeleCore.TeleDefExtension">
				<addCustomTick>true</addCustomTick>
			</li>
			<li Class="TeleCore.SubMenuExtension">
				<groupDef>TestGroup</groupDef>
				<category>Structure</category>
			</li>
		</modExtensions>
	</ThingDef>
	
	<ThingDef ParentName="Tele_StationBase">
		<defName>AtmosphericVentIn</defName>
		<label>atmospheric vent (intake)</label>
		<description>.</description>
		<thingCategories Inherit="false" />
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<drawerType>MapMeshAndRealTime</drawerType>
		<tickerType>Normal</tickerType>
		<uiIconPath>Buildings/Network/Intake/icon</uiIconPath>
		<designationCategory>Atmospheric</designationCategory>
		<rotatable>false</rotatable>
		<graphicData>
			<texPath>Buildings/Network/Intake/base</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(1.25,1.25)</drawSize>
			<drawOffset>(0,0,0.1)</drawOffset>
		</graphicData>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToBuild>1500</WorkToBuild>
			<Flammability>1.0</Flammability>
			<Beauty>-10</Beauty>
		</statBases>
		<size>(1,1)</size>
		<costList>
			<Steel>50</Steel>
			<ComponentIndustrial>1</ComponentIndustrial>
		</costList>
		<comps>
			<li Class="TAE.CompProperties_ANS_AirVent">
				<ventMode>Intake</ventMode>
				<gasThroughPut>10</gasThroughPut>
				<filter>
					<acceptedTag>Air</acceptedTag>
				</filter>
				<networks>
					<li>
						<networkDef>AtmosphericNetwork</networkDef>
						<roles>
							<li>Transmitter</li>
							<li>Producer</li>
							<li>Storage</li>
						</roles>
						<volumeConfig>
							<capacity>100</capacity>
							<area>1</area>
							<elevation>0</elevation>
							<height>1</height>
							<values>
								<fromCollection>AtmosphericNetwork</fromCollection>
							</values>
						</volumeConfig>
					</li>
				</networks>
				<generalIOConfig>
					<pattern>#O#|O#O|#O#</pattern>
				</generalIOConfig>
			</li>
			<li Class="TeleCore.CompProperties_FX"> 
				<fxLayers>
					<li>
						<categoryTag>AirVent</categoryTag>
						<layerTag>AirVentRotationLayer</layerTag>
						<fxMode>Dynamic</fxMode>
						<graphicData>
							<texPath>Buildings/Network/Intake/fan</texPath>
							<graphicClass>Graphic_Single</graphicClass>
							<drawSize>(1.25,1.25)</drawSize>
							<drawOffset>(0,0,0.2)</drawOffset>
						</graphicData>
						<rotate>
							<rotationSpeed>75</rotationSpeed>
						</rotate>
						<needsPower>false</needsPower>
					</li>
					<li>
						<categoryTag>AirVent</categoryTag>
						<graphicData>
							<texPath>Buildings/Network/Intake/top</texPath>
							<graphicClass>Graphic_Single</graphicClass>
							<drawOffset>0,0,0.1)</drawOffset>
							<drawSize>(1.25,1.25)</drawSize>
						</graphicData>
						<needsPower>false</needsPower>
					</li>
				</fxLayers>
			</li>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>45</basePowerConsumption>
			</li>
		</comps>
		<placeWorkers>
			<li>TAE.PlaceWorker_AtmosphericVent</li>
		</placeWorkers>
		<castEdgeShadows>false</castEdgeShadows>
		<staticSunShadowHeight>0</staticSunShadowHeight>	
		<fillPercent>1</fillPercent>		
		<surfaceType>Item</surfaceType>
		<modExtensions>
			<li Class="TeleCore.SubMenuExtension">
				<groupDef>TestGroup</groupDef>
				<category>Structure</category>
			</li>
		</modExtensions>
	</ThingDef>
	
